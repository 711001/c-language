#include <stdio.h>

int main()
{
    char a[7];
    
    while(scanf("%s",a)!=EOF){
        for (int i=0;i<6;i++){
            printf("%d",abs(a[i]-a[i+1]));
        }
        
        printf("\n");
            
    }
        
    return 0;
}
    
    
    //  https://zerojudge.tw/ShowProblem?problemid=a065
