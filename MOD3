#include <stdio.h>

int main()
{
    int k,n,i;
    int count[3];
    
    while(scanf("%d",&n)!=EOF){
        for (i=0;i<3;i++){
            count[i]=0;
        }
        
        for (i=0;i<n;i++){
            scanf("%d",&k);
            count[k%3]++;
        }
        
        printf("%d %d %d\n",count[0],count[1],count[2]);
            
    }
              

    return 0;
    
}
    
    
  //https://zerojudge.tw/ShowProblem?problemid=a058
