#include <stdio.h>
int fact(int i){
    int sum;
    if(i==0){
        sum=1;
    }
    else{
        sum=i*fact(i-1);
    }
    return sum;
}
int main()
{
    for(int i=0;i<=10;i++){
        printf("%d!=%d\n",i,fact(i));
    }
    return 0;
}

