#include <stdio.h>
#include<math.h>
int main(){
    int m,k,i;
    scanf("%d",&m);
    k=(int)sqrt(m);
    for ( i=2;i<=k;i++){
        if (m%i==0){
            break;
        }
    }
    if(i>k){
        printf("%d是素數",m);
    }
    else {
        printf("%d不是素數",m);
    }
    return 0;
}

