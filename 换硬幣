#include <stdio.h>

int main()
{
    int x,y,z,count;
    count =0;
    for(x=0;x<=50;x+=10){
        for(y=0;y<=50-x;y+=5){
            for(z=0;z<=50-x-y;z+=1){
                if(50==x+y+z){
                    count++;
                }
            }
        }
    }
    printf("%d",count);



    return 0;
}

