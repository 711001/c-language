#include<stdio.h>

int main(){
    long int sum=9;
    int a,b;
    scanf("%d",&a);
    for(b=1;;b++){
        if(sum%a==0){
            break;
        }
        else{
            sum=10*sum+9;
        }
    }
    printf("%ld",sum);
    return 0;
}

