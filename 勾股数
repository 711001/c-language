#include<math.h>
#include <stdio.h>

int main()
{
    
    int a,b,c;
    for(a=1;a<=100;a++){
        for(b=1;b<=100;b++){
            c=(int)sqrt(a*a+b*b);
            if(c*c==a*a+b*b && a+b>c && a+c>b && b+c>a&&c<=100&&a<b&&b<c){
                printf("%4d%4d%4d\n",a,b,c);
            }
        }
    }
    
    
    return 0;
}
