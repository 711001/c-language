#include <stdio.h>

int main()
{
    int i,j,k ,count=0;
    for(i=1;i<5;i++){
        for(j=1;j<5;j++){
            for(k=1;k<5;k++){
                if(i!=k && i!=j && j!=k){
                    count++;
                    printf("%d%d%d ",i,j,k);
                    if (count%8==0){
                        printf("\n");
                    }
                }
            }
        }
    }
    printf("total is %d",count);
    return 0;
}

