#include <stdio.h>

int main()
{
    int a,b,temp;
    scanf("%d%d",&a,&b);
    if (b>a){
        temp=a;
        a=b;
        b=temp;
    }
    for(int i =a;i>0;i++){
        if (i%a==0&&i%b==0){
            printf("%d",i);
            break;
        }
    }

    return 0;
}

