#include<math.h>
#include <stdio.h>
int num[400];
int prime(int mp){
    for(int i=2;i<=(int)sqrt((double)mp+1);i++){
        if(mp%i==0){
            return 0;
        }
    }
    return 1;
}
int main()
{
    
    int i,mp,j=0,n=0,count=0;
    for(i=3;i<=1993;i+=2){
        
        if(prime(i)){
            num[j++]=i;
             
        }
    }
    
    for(j--; num[j]>1898; j--)  
    {
        for(i=0; num[j]-num[i]>1898; i++);
        if(num[j]-num[i] == 1898)  
        printf("(%d).%3d,%d\n", ++count, num[i], num[j]);
         
        
    }
    
    
    return 0;
}

